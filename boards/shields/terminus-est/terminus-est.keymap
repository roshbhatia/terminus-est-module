        default_layer {
            display-name = "Base";
            bindings = <
            //  COL00     COL01     COL02     COL03     COL04     COL05     COL06     COL07     COL08     COL09     COL10     COL11     COL12     COL13     COL14     COL15     COL16     COL17     COL18     COL19     COL20     COL21     // ROW
                          &kp GRAVE &kp N1    &kp N2    &kp N3    &kp N4    &kp N5    &kp N6                                                                          &kp N7    &kp N8    &kp N9    &kp N0    &kp MINUS &kp PLUS  &kp BSPC  // ROW0
                &kp TAB   &kp TAB   &kp Q     &kp W     &kp E     &kp R     &kp T                         &kp ESC                       &kp ESC             &kp Y     &kp U     &kp I     &kp O     &kp P     &kp LBKT  &kp RBKT  &kp BSPC  // ROW1
                &kp LCTL  &kp RCTL  &kp A     &kp S     &kp D     &kp F     &kp G                         &kp LALT                      &kp LALT            &kp H     &kp J     &kp K     &kp L     &kp SEMI  &kp SQT   &kp BLSH            // ROW2
                &kp LSHFT &kp RSHFT &kp Z     &kp X     &kp C     &kp V     &kp B                         &kp LGUI                      &kp LGUI            &kp B     &kp N     &kp M     &kp COMMA &kp DOT   &kp FLSH  &kp RET             // ROW3
